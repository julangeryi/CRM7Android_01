        </activity>
        <activity
            android:name=".TransportActivity"
            android:icon="@drawable/steel"
            android:windowSoftInputMode="stateHidden|stateUnchanged"
            android:label="@string/title_activity_transport" >
        </activity>
        <activity
            android:name=".CheckWareHouseActivity"
             android:icon="@drawable/steel"
            android:windowSoftInputMode="stateHidden|stateUnchanged"
            android:label="@string/title_activity_check_ware_house" >
