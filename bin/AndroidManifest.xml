        <activity
            android:name=".OffLineMainActivity"
             android:icon="@drawable/steel"
            android:windowSoftInputMode="stateHidden|stateUnchanged"
            android:label="@string/title_activity_off_line_main" >
        </activity>
        <activity
            android:name=".OffLineCheckWareHouseActivity"
             android:icon="@drawable/steel"
            android:windowSoftInputMode="stateHidden|stateUnchanged"
            android:label="@string/title_activity_off_line_check_ware_house" >
